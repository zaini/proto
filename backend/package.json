{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "zaini",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .env.development -- nodemon -e ts --exec \"yarn run start\"",
    "test:dev": "dotenv -e .env.test -- nodemon -e ts --exec \"yarn run start\"",
    "start": "dotenv -e .env.development -- tsc && node dist/server.js",
    "test:start": "dotenv -e .env.test -- tsc && node dist/server.js",
    "db:migrate": "dotenv -e .env.development -- npx prisma migrate dev && npx prisma generate",
    "test:db:migrate": "dotenv -e .env.test -- npx prisma migrate dev",
    "db:seed": "dotenv -e .env.development -- npx prisma migrate reset --force",
    "test:db:seed": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset --force --skip-seed",
    "test:db:reset": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed",
    "db:setup": "yarn run db:reset && yarn run db:migrate",
    "test:db:setup": "yarn run test:db:reset && yarn run test:db:migrate",
    "test": "dotenv -e .env.test jest --verbose --runInBand -i",
    "build": "tsc",
    "generate-gql-types": "graphql-codegen"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@prisma/client": "^3.6.0",
    "@types/express": "^4.17.13",
    "apollo-server": "^3.5.0",
    "argon2": "^0.28.3",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^16.0.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.0",
    "passport-github2": "^0.1.12",
    "typescript": "^4.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@codejedi365/jest-serial-runner": "^2.0.0",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/passport": "^1.0.7",
    "@types/passport-github2": "^1.2.5",
    "@types/winston": "^2.4.4",
    "babel-jest": "^27.5.1",
    "dotenv-cli": "^5.0.0",
    "jest": "^27.5.1",
    "jest-runner": "^27.5.1",
    "nodemon": "^2.0.14",
    "prisma": "^3.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "xmlhttprequest": "^1.8.0"
  }
}